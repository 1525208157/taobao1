function promoted(flag){
    var hasbeen = 0;
    var notbeen = 0;
    var ids = document.forms['open'].elements['selectedIds'];
    var type = typeof(ids.name);

    if ( type != 'undefined' ){

        if( ids.checked ){
            if( isRecommended(ids.value) ){
                hasbeen++;
                if( flag == 1 ){
                    ids.checked = false;
                }
            }
            else{
                notbeen++;
                if( flag == 0 ){
                    ids.checked = false;
                }
            }
        }
    }
    else{
        for(i=0; i<ids.length; i++){

            if( ids[i].checked ){
                if( isRecommended(ids[i].value) ){
                    hasbeen++;
                    if( flag == 1 ){
                        ids[i].checked = false;
                    }
                }
                else{
                    notbeen++;
                    if( flag == 0 ){
                        ids[i].checked = false;
                    }
                }
            }
        }
    }
    if( flag == 0 ){
        if( hasbeen == 0 ){
            if( notbeen > 0 ){
                alert("该宝贝未被推荐或已设置精品橱窗，请选择其他需要取消推荐的宝贝！");
            }
            else{
                alert("请选中要取消推荐的宝贝，精品橱窗请到橱窗推荐宝贝页面进行设置！");
            }
            return false;
        }
    }
    else{
        if( notbeen == 0 ){
            if( hasbeen > 0 ){
                alert("该宝贝已被推荐，请选择未被推荐的宝贝！");
            }
            else{
                alert("请选中要推荐的宝贝！");
            }
            return false;
        }
		if (document.getElementById("J_maxRecommendNum_hidden") && document.getElementById("J_curRecommendNum_1")) {
            var maxRecommendNumTmp = document.getElementById("J_maxRecommendNum_hidden").value;
            var maxRecommendNumStr = maxRecommendNumTmp.replace(/\D/g,"");
            var maxRecommendNum = parseInt(maxRecommendNumStr);
            var curRecommendNumTmp = document.getElementById("J_curRecommendNum_1").innerHTML;
            var curRecommendNumStr = curRecommendNumTmp.replace(/\D/g, "");
            var curRecommendNum = parseInt(curRecommendNumStr);
            var remainRecommendNum = maxRecommendNum - curRecommendNum;
            if( notbeen > remainRecommendNum ){
                alert("您最多只能再推荐 " + remainRecommendNum + " 件宝贝！");
                return false;
            }
		}
    }
